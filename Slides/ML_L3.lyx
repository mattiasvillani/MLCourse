\begin_deeper
\begin_layout Itemize
Parametric model 
\begin_inset Formula $y\vert\boldsymbol{x}\sim f_{\boldsymbol{\theta}}(y\vert\boldsymbol{x})$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Learn the parameters by minimizing a cost function
\begin_inset Formula 
\[
\hat{\boldsymbol{\theta}}=\arg\underset{\boldsymbol{\theta}}{\min}\underset{\text{cost function}J(\boldsymbol{\theta})}{\underbrace{\frac{1}{n}\sum_{i=1}^{n}\overset{\text{loss function}}{\overbrace{L\left(\hat{y}(\boldsymbol{x}_{i};\boldsymbol{\theta}),y_{i}\right)}}}}
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Squared error
\series default
\color inherit
 for regression: 
\begin_inset Formula 
\[
L\left(\hat{y}(\boldsymbol{x}_{i};\boldsymbol{\theta}),y_{i}\right)=\left(\hat{y}(\boldsymbol{x}_{i};\boldsymbol{\theta})-y_{i}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Itemize
Squared error loss = negative Gaussian log-likelihood
\begin_inset Formula 
\[
\log p(y_{i}\vert\boldsymbol{x}_{i},\boldsymbol{\beta})=-\frac{1}{2}\log(2\pi\sigma^{2})-\frac{1}{2\sigma^{2}}\left(y_{i}-\mathbf{x}_{i}^{\top}\boldsymbol{\beta}\right)^{2}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout

\series bold
\color orange
Loss functions and cost functions
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Learn the parameters by minimizing a cost function
\begin_inset Formula 
\[
\hat{\boldsymbol{\theta}}=\arg\underset{\boldsymbol{\theta}}{\min}\underset{\text{cost function}J(\boldsymbol{\theta})}{\underbrace{\frac{1}{n}\sum_{i=1}^{n}\overset{\text{loss function}}{\overbrace{L\left(\hat{y}(\boldsymbol{x}_{i};\boldsymbol{\theta}),y_{i}\right)}}}}
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Cross-entropy
\series default
\color inherit
 for binary classification 
\begin_inset Formula $y\in\{0,1\}$
\end_inset


\size footnotesize

\begin_inset Formula 
\[
L\left(\hat{y}(\boldsymbol{x}_{i};\boldsymbol{\theta}),y_{i}\right)=-y_{i}\log\mathrm{Pr}(y_{i}=1\vert\boldsymbol{x}_{i},\theta)-(1-y_{i})\log\mathrm{Pr}(y_{i}=0\vert\boldsymbol{x}_{i},\theta)
\]

\end_inset


\end_layout

\begin_layout Itemize
Cross-entropy loss = negative Bernoulli log-likelihood
\size footnotesize

\begin_inset Formula 
\[
\mathrm{Pr}(Y_{i}=y_{i}\vert\boldsymbol{x}_{i},\boldsymbol{\beta})=\mathrm{Pr}(y_{i}=1\vert\boldsymbol{x}_{i},\boldsymbol{\beta})^{y_{i}}\mathrm{Pr}(y_{i}=0\vert\mathbf{x}_{i},\boldsymbol{\beta})^{1-y_{i}}
\]

\end_inset


\size default
where for logistic regression 
\size footnotesize

\begin_inset Formula 
\[
\mathrm{Pr}(y_{i}=1\vert\boldsymbol{x}_{i},\boldsymbol{\beta})=\frac{\exp(\mathbf{x}_{i}^{T}\boldsymbol{\beta})}{1+\exp(\mathbf{x}_{i}^{T}\boldsymbol{\beta})}.
\]

\end_inset


\end_layout

\begin_layout Itemize
See Bonus slides for a little more on (cross-)entropy.
 
\end_layout

